			o.value = l
			kwl.add o
		elseif c2 = c and t2 = "class" then
			set o = document.createElement("option")
			o.text = n2
			o.disabled = true
			o.value = l
			kwl.add o

		end if

	next


	
end function

function setCaret(obj, position) 
	dim range
	set range = obj.createTextRange()
	range.move "character", position
	range.select()
end function


sub openFile



	dim fPath
	fPath = selectFile
	if fPath = "" then exit sub


	

	dim fso 
	set fso = createObject("Scripting.FileSystemObject")
	dim f
	set f = fso.openTextFile(fPath)
	dim fc
	if f.atEndOfStream then exit sub
	fc = f.readAll
	f.close
	set f = nothing
	set fso = nothing

	dim ed
	set ed = getEditor
	ed.value = fc
	
	loadStructure	

	getExec.value = fPath
end sub

