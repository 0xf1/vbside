sub executeFile
	dim exec
	set exec = getExec

	dim objShell
	set objShell = CreateObject("shell.application")

	dim par
	if ubound(split(exec.value," "))>0 then
		par = mid( exec.value,  instr(exec.value," ")+1,  len(exec.value)  )
	end if


        objShell.ShellExecute split(exec.value," ")(0), par, "", "open", 1

        set objShell = nothing

end sub

