function joinAt(joinArray, joinAtIndex, joinWith)
	dim arLen: arLen = ubound(joinArray)

	for joinAtIndex = joinAtIndex to arLen
		joinAt = joinAt & joinArray(joinAtIndex) 
		if arLen <> joinAtIndex then joinAt = joinAt & joinWith
	next
end function

